<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.andrea.competitionapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/launch_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/launch_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListActivity" />
        <activity android:name=".AddEventsActivity" />
        <activity android:name=".CompetitionInformation" />
        <activity android:name=".AdminListView" />
        <activity android:name=".AdminCompInfo"></activity>
    </application>

</manifest>